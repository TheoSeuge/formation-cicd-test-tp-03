meta {
  name: Health Check
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/health
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body.status: eq UP
}

tests {
  test("should return status 200", function() {
    expect(res.getStatus()).to.equal(200);
  });

  test("should have status UP", function() {
    expect(res.getBody().status).to.equal("UP");
  });
}
